<header-view songsData="{{songsData}}" />

<view class="song-list">
  <view wx:for="{{songsData.tracks}}" wx:key="id" class="border">
    <song-item
      songItem="{{item}}"
      index="{{index + 1}}"
      showImg="{{false}}"
      showIndex
      bind:tap="handleSavePlayList"
    />
  </view>
</view>
